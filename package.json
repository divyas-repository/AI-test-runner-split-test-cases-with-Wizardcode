{
  "name": "ai-test-runner",
  "version": "1.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "auto-generate": "ts-node src/auto-csv-generator.ts",
    "template-generate": "ts-node src/template-generator.ts",
    "implement-missing": "ts-node src/missing-test-generator.ts",
    "implement-pattern": "ts-node src/pattern-based-generator.ts",
    "watch-csv": "ts-node src/csv-watcher.ts",
    "auto-cleanup": "ts-node src/auto-cleanup.ts",
    "generate-clean": "npm run template-generate && npm run auto-cleanup",
    "test-clean": "npm test && npm run auto-cleanup",
    "build-clean": "npm run build && npm run auto-cleanup"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "ejs": "^3.1.8",
    "gpt4all": "^4.0.0",
    "open": "^9.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@types/ejs": "^3.1.5",
    "@types/glob": "^8.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}